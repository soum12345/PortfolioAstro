---
import BaseLayout from "../layouts/BaseLayout.astro";
import HorizontalCard from "../components/HorizontalCard.astro";
---

<BaseLayout title = "Projects" sideBarActiveItemID="projects">
  <div>
    <div class="text-3xl w-full font-bold mb-5">Projects universitaires</div>
  </div>

  <HorizontalCard
    title="Création d'une application de gestion des biens immobiliers"
    img="/SAE_BUT2.png"
    desc="SAE BUT2"
    url="#"
    badge="JAVA , OracleSQL, PLSQL, WindowBuilder"
  />

  <div class="divider my-0"></div>

  <HorizontalCard
    title="Algorithme de résolution de sudoku"
    img="/SAE_BUT1_sudoku.png"
    desc="SAE BUT1"
    url="#"
    badge="Ada"
  />

  <div class="divider my-0"></div>

  <HorizontalCard
    title="Installation et configuration d'un poste de travail"
    img="/SAE_BUT1_machinevirtuel.png"
    desc="SAE BUT1"
    url="#"
    badge="Owncload"
  />

  <div class="divider my-0"></div>

  <HorizontalCard
    title="Visualisation de données scientifiques les données 'brutes'"
    img="/SAE_BUT2-4.png"
    desc=" SAE BUT2 semestre 4"
    url="#"
    badge="Dgango, MongoDB, Streamlit"
  />

  <div class="divider my-0"></div>

  <HorizontalCard
    title="Organisation d'un triathlon"
    img="/SAE_BUT1S2_cachier_charges.png"
    desc="SAE BUT1 semestre 2"
    url="#"
    badge="Grap"
  />
  
  <div class="divider my-0"></div>
  <HorizontalCard
    title="Travaille en équipe pour élaboration d'un modèle d'intelligence artificielle"
    img="/SAE_BUT2-3.png"
    desc="SAE BUT3 semestre 6"
    url="#"
    badge="Python, Skelearn, Pytorch, Bert"
  />
</BaseLayout>
